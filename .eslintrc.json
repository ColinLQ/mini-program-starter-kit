{
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jquery": true,
    "mocha": true
  },
  "rules": {
    "eqeqeq": 2,
    "quotes": 0,
    "no-console": 1,
    "no-debugger": 1,
    "no-var": 2,
    "import/named": 0,
    "import/no-unresolved": 0,
    "semi": [
      0,
      "always"
    ],
    "no-trailing-spaces": 0,
    "eol-last": 0,
    "no-underscore-dangle": 0,
    "no-alert": 0,
    "no-lone-blocks": 0,
    "jsx-quotes": 2,
    "import/extensions": 1
  },
  "globals": {
    "App": true,
    "wx": true,
    "getApp": true,
    "Page": true,
    "getCurrentPages": true
  }
}
